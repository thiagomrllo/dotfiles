{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",

  "logo": {
    "type": "builtin",
    "name": "fedora",
    "padding": {
      "top": 1
    }
  },

  "display": {
    "separator": "  "
  },

  "modules": [
    "os",
    "kernel",
    "packages",
    "shell",
    "wm",
    "uptime",
    "osAge",
    {
      "type": "memory",
      "format": "{used} / {total} ({percentage}%)"
    }
  ]
}
